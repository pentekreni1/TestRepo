<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.StartPageController"
            prefHeight="606.0" prefWidth="799.0">
    <children>
        <ImageView fx:id="background" fitHeight="593.0" fitWidth="799.0" layoutY="13.0" pickOnBounds="true">
            <image>
                <Image url="@../images/background.jpg"/>
            </image>
        </ImageView>
        <Button fx:id="ButtonHH" layoutX="300.0" layoutY="200.0" mnemonicParsing="false" onAction="#startHumanHuman"
                text="Human-Human" textOverrun="CLIP">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
            <effect>
                <SepiaTone level="0.51"/>
            </effect>
        </Button>
        <Button fx:id="ButtonHC" layoutX="300.0" layoutY="300.0" mnemonicParsing="false" onAction="#startHumanComputer"
                text="Human-Computer" textOverrun="CLIP">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
            <effect>
                <SepiaTone level="0.51"/>
            </effect>
        </Button>
    </children>
</AnchorPane>
